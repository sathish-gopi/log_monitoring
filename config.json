{
        "agent": {
                "metrics_collection_interval": 1,
                "run_as_user": "ubuntu"
        },
        "logs": {
                "logs_collected": {
                        "files": {
                                "collect_list": [
                                        {
                                                "file_path": "/var/log/clipflip/vps.log",
                                                "log_group_name": "vt1-server-log-group",
                                                "log_stream_name": "{instance_id}",
                                                "retention_in_days": 1
                                        }
                                ]
                        }
                }
        },
        "metrics": {
                "metrics_collected": {
                        "mem": {
                                "measurement": [
                                        "mem_used_percent"
                                ],
                                "metrics_collection_interval": 1
                        }
                       
                }
        }
}
